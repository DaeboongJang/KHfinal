<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
	PUBLIC "-//mybatis.org/DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<!-- 기존에는 settings -null값 / 별칭 설정 / mapper클래스 등록 / DB설정 스르핑에서는 settings 
		-null값 / 별칭 설정 까지만 -->
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	<typeAliases>
		<typeAlias type="kh.com.finalProject.board.BoardDTO" alias="BoardDTO" />
		<typeAlias type="kh.com.finalProject.files.FileDTO" alias="FileDTO" />
		<typeAlias type="kh.com.finalProject.reply.ReplyDTO" alias="ReplyDTO" />
		<typeAlias type="kh.com.finalProject.likes.LikesDTO" alias="LikesDTO" />
		<typeAlias type="kh.com.finalProject.follow.FollowDTO" alias="FollowDTO" />
		<typeAlias type="kh.com.finalProject.report.ReportDTO" alias="ReportDTO" />
		<typeAlias type="kh.com.finalProject.member.MemberDTO" alias="MemberDTO" />
		<typeAlias type="kh.com.finalProject.note.NoteDTO" alias="NoteDTO"/>
		<typeAlias type="kh.com.finalProject.publicdata.DataDTO" alias="DataDTO"/>
		<typeAlias type="kh.com.finalProject.publicdata.FestivalDTO" alias="FestivalDTO"/>
		<typeAlias type="kh.com.finalProject.publicdata.LeportsDTO" alias="LeportsDTO"/>
		<typeAlias type="kh.com.finalProject.visit.VisitDTO" alias="VisitDTO"/>
		<typeAlias type="kh.com.finalProject.publicdata.SearchDTO" alias="SearchDTO"/>
	</typeAliases>

	<!-- <mappers>
		<mapper resource="mappers/file-mapper.xml" />
		<mapper resource="mappers/board-mapper.xml" />
		<mapper resource="mappers/member-mapper.xml" />
		<mapper resource="mappers/reply-mapper.xml" />
	</mappers>  -->

</configuration>